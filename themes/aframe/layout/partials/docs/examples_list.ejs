<% if (page.examples) { %>
  <div id="doc-examples-list" class="nav-list">
    <p class="section-title docs-section-title">예제</p>

    <% if (page.examples.length) { %>
      <ul class="subnav">
        <% page.examples.forEach(function (example) { %>
          <li class="subnav-item">
            <a class="subnav-link" href="<%- example.src %>">
              <%- example.title.replace('<', '&lt;').replace('>', '&gt;') %>
            </a>
          </li>
        <% }); %>
      </ul>
    <% } else { %>
      <ul class="subnav">
        <li class="subnav-item">
          <a class="subnav-link" href="<%- website_github_edit_url(page.path) %>" style="font-style: italic">
            이 페이지에는 더 이상 예제가 없습니다.
          </a>
        </li>
      </ul>
    <% } %>
  </div>
<% } %>
